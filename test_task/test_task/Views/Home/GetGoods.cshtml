@model IQueryable<test_task.Models.Goods>
<table class="table table-hover table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var goods in Model)
        {
            <tr>
                <td>@goods.Name</td>
                <td>@goods.Description</td>
            </tr>
        }
    </tbody>
</table>
<a id="addGoods" href="\Home\AddGoods?id=@ViewBag.ID" class="btn btn-sm">Add new goods</a>